[PrefixDeclaration]
rdf:        http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:       http://www.w3.org/2000/01/rdf-schema#
owl:        http://www.w3.org/2002/07/owl#
xsd:        http://www.w3.org/2001/XMLSchema#
blon:       http://www.semanticblockchain.com/Blondie.owl#

[MappingDeclaration] @collection [[
mappingId	MAPPING-TRON-TRANSFER
target		<http://www.semanticblockchain.com/resource/TronTx_{id}> a blon:TronTransaction ; blon:txIdTronTransaction {id}^^xsd:string ; blon:senderTronTransaction {from}^^xsd:string ; blon:recipientTronTransaction {to}^^xsd:string ; blon:contractAddressTronTransaction {contract_address}^^xsd:string ; blon:valueTronTransaction {amount}^^xsd:decimal ; blon:timestampTronTransaction {timestamp}^^xsd:string ; blon:blockNumberTronTransaction {block_number}^^xsd:integer . 
source		SELECT id, MAX("from") AS "from", MAX("to") AS "to", MAX(contract_address) AS contract_address, MAX(amount) AS amount, MAX(timestamp) AS timestamp, MAX(block_number) AS block_number FROM transfer GROUP BY id LIMIT 10
]]

[MappingDeclaration] @collection [[
mappingId	MAPPING-TRON-CONTRACT-EXEC
target		<http://www.semanticblockchain.com/resource/ContractExec_{id}> a blon:TronContractExecution ; blon:txIdTronTransaction {tx}^^xsd:string ; blon:contractAddressTronTransaction {contract_address}^^xsd:string ; blon:timestampTronTransaction {timestamp}^^xsd:string ; blon:blockNumberTronTransaction {block_number}^^xsd:integer ; blon:callerTronContractExecution {caller}^^xsd:string ; blon:methodIdTronContractExecution {method_id}^^xsd:string . 
source		SELECT id, tx, contract_address, timestamp, block_number, caller, method_id FROM contract_execution LIMIT 10
]]
