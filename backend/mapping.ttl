@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix blon: <http://www.semanticblockchain.com/Blondie.owl#> .
@prefix obda: <https://w3id.org/obda/vocabulary#> .

<urn:r2rml:MAPPING-TRON-TRANSFER> a rr:TriplesMap;
  rr:predicateObjectMap [ a rr:PredicateObjectMap;
      rr:predicate blon:timestampTronTransaction;
      rr:objectMap [ a rr:TermMap, rr:ObjectMap;
          rr:termType rr:Literal;
          rr:column "timestamp";
          rr:datatype xsd:dateTime
        ]
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:termType rr:Literal;
          rr:datatype xsd:integer;
          rr:column "block_number"
        ];
      rr:predicate blon:blockNumberTronTransaction
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:column "id";
          rr:datatype xsd:string;
          rr:termType rr:Literal
        ];
      rr:predicate blon:txIdTronTransaction
    ], [ a rr:PredicateObjectMap;
      rr:predicate blon:senderTronTransaction;
      rr:objectMap [ a rr:TermMap, rr:ObjectMap;
          rr:datatype xsd:string;
          rr:termType rr:Literal;
          rr:column "from"
        ]
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:datatype xsd:decimal;
          rr:column "amount";
          rr:termType rr:Literal
        ];
      rr:predicate blon:valueTronTransaction
    ], [ a rr:PredicateObjectMap;
      rr:objectMap [ a rr:ObjectMap, rr:TermMap;
          rr:datatype xsd:string;
          rr:column "to";
          rr:termType rr:Literal
        ];
      rr:predicate blon:recipientTronTransaction
    ], [ a rr:PredicateObjectMap;
      rr:predicate blon:contractAddressTronTransaction;
      rr:objectMap [ a rr:TermMap, rr:ObjectMap;
          rr:termType rr:Literal;
          rr:datatype xsd:string;
          rr:column "contract_address"
        ]
    ];
  rr:logicalTable [ a rr:R2RMLView;
      rr:sqlQuery "SELECT * FROM transfer"
    ];
  rr:subjectMap [ a rr:SubjectMap, rr:TermMap;
      rr:template "http://www.semanticblockchain.com/resource/TronTx_{id}";
      rr:termType rr:IRI;
      rr:class blon:TronTransaction
    ] .
